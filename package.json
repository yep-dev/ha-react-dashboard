{
  "name": "ha-react-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "VITE_HA_URL=${HA_URL} vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "barrels": "barrelsby -DS -c barrels.json",
    "sync": "hakit-sync-types --token $HA_TOKEN --url $HA_URL",
    "types": "./node_modules/.bin/ts-node --esm sync-types.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hakit/core": "1.0.14",
    "@iconify/react": "^4.1.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.0",
    "ha-component-kit": "^1.0.14",
    "home-assistant-js-websocket": "^8.2.0",
    "javascript-time-ago": "^2.5.9",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "use-debounce": "^9.0.4"
  },
  "devDependencies": {
    "@types/node": "^20.5.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "barrelsby": "^2.8.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.0.2",
    "standard": "^17.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.2.0",
    "ws": "^8.13.0"
  }
}
